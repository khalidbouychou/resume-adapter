# Backend configuration

# Primary provider (Hugging Face Inference)
HUGGINGFACE_API_KEY=HUGGINGFACE_API_KEY
HUGGINGFACE_MODEL=MODEL_NAME
HF_MAX_NEW_TOKENS=2048
HF_TEMPERATURE=0.7

# OpenRouter (OpenAI-compatible)
OPENROUTER_API_KEY=
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL=openai/gpt-4o-mini
OPENROUTER_REFERER=
OPENROUTER_TITLE=

# OpenAI (optional fallback)
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4o-mini

# DeepSeek (optional fallback)
DEEPSEEK_API_KEY=
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat

# xAI Grok (optional fallback)
XAI_API_KEY=
XAI_BASE_URL=https://api.x.ai/v1
XAI_MODEL=grok-2-mini

# Provider order (comma-separated). First available provider is used.
PROVIDER_ORDER=huggingface,openrouter,openai,deepseek,grok
GOOGLE_API_KEY=GOOGLE_API_KEY 